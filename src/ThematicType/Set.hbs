<span class='spanTemplate'>
	<span class='title'>
		{{lang data.type}}
	</span>
	{{#if data.remark}}
		<span class='subtitle'>
			{{data.remark}}
		</span>
	{{/if}}
	<span class='subtitle'>
		{{#if data.startDate}}
			{{Date data.startDate}} - 
		{{/if}}

		{{#if data.endDate}}
			{{Date data.endDate}}
		{{/if}}
	</span>
</span>