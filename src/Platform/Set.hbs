{{Image data.platform.image}}
<span class='spanTemplate'>
	<span class='title'>{{data.platform.name}}</span>
	<span class='subtitle'>
		{{data.contact.firstname}} {{data.contact.surname}} - {{lang data.role}}
	</span>
	<span class='subtitle'>
		{{#if data.startDate}}
			{{Date data.startDate}} -
		{{/if}}

		{{#if data.endDate}}
			{{Date data.endDate}}
		{{/if}}
	</span>
</span>
