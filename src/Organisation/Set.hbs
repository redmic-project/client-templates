{{ImageDetailsPublic data.organisation.logo}}
<span class='spanTemplate'>
	<span class='title'>
		{{data.organisation.name}}{{#if data.organisation.acronym}} ({{data.organisation.acronym}}){{/if}}
	</span>
	<span class='subtitle'>
		{{lang data.role}}
	</span>
	<span class='subtitle'>
		{{#if data.startDate}}
			{{Date data.startDate}} - 
		{{/if}}

		{{#if data.endDate}}
			{{Date data.endDate}}
		{{/if}}
	</span>
</span>
